{
    "name": "atlas/hermes-cms-bundle",
    "type": "symfony-bundle",
    "description": "CMS based on Symfony ",
    "keywords": ["CMS", "Symfony" ,"backend", "frontend", "admin", "generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hermes - Tayeb Chikhi"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.*",
        "doctrine/doctrine-bundle": "2.*",
        "doctrine/doctrine-migrations-bundle": "3.*",
        "doctrine/migrations": "3.*",
        "doctrine/orm": "2.*",
        "helios-ag/fm-elfinder-bundle": "12.*",
        "liip/imagine-bundle": "2.*",
        "phpdocumentor/reflection-docblock": "5.*",
        "select2/select2": "4.*",
        "symfony/asset": "7.2.*",
        "symfony/cache": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/dependency-injection": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/flex": "1.*",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/http-kernel": "7.2.*",
        "symfony/intl": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/messenger": "7.2.*",
        "symfony/monolog-bundle": "3.*",
        "symfony/polyfill-php72": "1.x-dev",
        "symfony/process": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/serializer": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/validator": "7.2.*",
        "symfony/web-link": "7.2.*",
        "symfony/webpack-encore-bundle": "2.*",
        "symfony/yaml": "7.2.*",
        "twig/cssinliner-extra": "3.*",
        "twig/extra-bundle": "3.*",
        "twig/inky-extra": "3.*",
        "twig/intl-extra": "3.*",
        "twig/string-extra": "3.*",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "2.5.*"
    },
    "require-dev": {
        "behat/behat": "^v3.11.0",
        "behat/mink-extension": "^2.3.1",
        "behat/mink-selenium2-driver": "^v1.6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friends-of-behat/mink": "^v1.10.0",
        "friends-of-behat/mink-browserkit-driver": "^v1.6.1",
        "friends-of-behat/mink-extension": "^2.7.2",
        "friends-of-behat/symfony-extension": "^v2.3.1",
        "friendsofphp/php-cs-fixer": "^v3.9.0",
        "liip/functional-test-bundle": "^4.4.3",
        "liip/test-fixtures-bundle": "^2.2.0",
        "phpunit/phpunit": "^9.3.20||^9.5.0",
        "symfony/browser-kit": "v7.2.0",
        "symfony/css-selector": "^7.2.0",
        "symfony/debug-bundle": "^7.2.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.2.0",
        "symfony/stopwatch": "^7.2.0",
        "symfony/web-profiler-bundle": "^7.2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*"
    },
    "scripts": {
        "auto-scripts": {
            "hermes:prepare-directories": "symfony-cmd",
            "d:s:u --em=default --force --complete ": "symfony-cmd",
            "d:s:u --em=config --force --complete ": "symfony-cmd",
            "hermes:db-update": "symfony-cmd",
            "elfinder:install": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "7.2.*"
        }
    }
}
