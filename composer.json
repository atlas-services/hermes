{
    "name": "atlas/hermes-cms-bundle",
    "type": "symfony-bundle",
    "description": "CMS based on Symfony ",
    "keywords": ["CMS", "Symfony" ,"backend", "frontend", "admin", "generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hermes - Tayeb Chikhi"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.x-dev",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "2.6.x-dev",
        "doctrine/doctrine-migrations-bundle": "^3.0@dev",
        "doctrine/migrations": "^3.0@dev",
        "doctrine/orm": "2.11.x-dev",
        "friendsofsymfony/ckeditor-bundle": "^2.3.0",
        "helios-ag/fm-elfinder-bundle": "^10.0",
        "liip/imagine-bundle": "^2.0@dev",
        "phpdocumentor/reflection-docblock": "5.x-dev",
        "select2/select2": "dev-master",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "5.4.*",
        "symfony/cache": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dependency-injection": "v5.4.2",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.11",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/http-kernel": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "3.5.0.*",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/webpack-encore-bundle": "v1.13.2",
        "symfony/yaml": "5.4.*",
        "twig/cssinliner-extra": "v3.3.5",
        "twig/extra-bundle": "3.3.*",
        "twig/inky-extra": "3.x-dev",
        "twig/intl-extra": "3.x-dev",
        "twig/string-extra": "3.x-dev",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.12.0",
        "behat/behat": "^v3.11.0",
        "behat/mink-extension": "^2.3.1",
        "behat/mink-goutte-driver": "^v1.3.0",
        "behat/mink-selenium2-driver": "^v1.6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "friends-of-behat/mink": "^v1.10.0",
        "friends-of-behat/mink-browserkit-driver": "^v1.6.1",
        "friends-of-behat/mink-extension": "^2.7.2",
        "friends-of-behat/symfony-extension": "^v2.3.1",
        "liip/functional-test-bundle": "4.4.3",
        "liip/test-fixtures-bundle": "1.10.1",
        "phpunit/phpunit": "9.5.25",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "6.1.x-dev",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "hermes:prepare-directories": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd",
            "elfinder:install": "symfony-cmd",
            "d:s:u --em=default --force --complete ": "symfony-cmd",
            "d:s:u --em=config --force --complete ": "symfony-cmd",
            "hermes:db-update": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.4.*"
        }
    }
}
