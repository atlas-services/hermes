<!-- Portfolio Grid -->
<section id="portfolio" class="section_content px-0" style="background-color:{{ content_bgcolor }}">
    <div class="container col-12 col-lg-{{ folio_container_width|col_lg() }}">
        <div class="row ml-5" id="folio">
        <div id="staggering">
            {% for post in  section.posts|sort((a, b) => a.position <=> b.position) %}
                {% if(vich_uploader_asset(post, 'imageFile') is not null) %}
                    <span class="staggering">
                        <img class="stagge" src="{{ asset(vich_uploader_asset(post, 'imageFile'))| imagine_filter('app_fixed_carre_150') }}"/>
                    </span>
                {% endif %}
            {% endfor %}
        </div>
    </div>
    </div>
</section>

