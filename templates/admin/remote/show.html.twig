<!DOCTYPE html>

<title>remote</title>

{% block body %}
    <h1 class="text-secondary">remote</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ remote.id }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ remote.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ remote.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ remote.name }}</td>
            </tr>
            <tr>
                <th>Summary</th>
                <td>{{ remote.summary }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('remote_index') }}">back to list</a>

    <a href="{{ path('remote_edit', {'id': remote.id}) }}">edit</a>

    {{ include('admin/remote/_delete_form.html.twig') }}
{% endblock %}
